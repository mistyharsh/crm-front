{
  "name": "@clobwise/crm-front",
  "version": "0.1.0",
  "description": "Frontend for CRM",
  "type": "module",
  "scripts": {
    "check:types": "tsc -b tsconfig.json",
    "dev": "rspack dev --env development",
    "dev:storybook": "storybook dev --ci -p 6006",
    "dev:gql": "npm run dev:gqlGen && npm run dev:gqlFormat",
    "dev:gqlGen": "NODE_OPTIONS='--import tsx' graphql-codegen --c ./script/Codegen.ts",
    "dev:gqlFormat": "prettier --write ./src/api",
    "dev:format": "prettier --write .",
    "build": "rspack build --env production",
    "build:storybook": "storybook build"
  },
  "imports": {
    "#api/*": "./src/api/*",
    "#base/*": "./src/base/*",
    "#shared/*": "./src/shared/*"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "volta": {
    "node": "22.6.0"
  },
  "dependencies": {
    "@mantine/core": "^7.12.2",
    "@mantine/dates": "^7.12.2",
    "@mantine/hooks": "^7.12.2",
    "@tanstack/react-form": "^0.21.0",
    "@tanstack/react-query": "^5.51.21",
    "@tanstack/react-router": "^1.45.11",
    "clsx": "^2.1.1",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "ky": "^1.6.0",
    "lucide-react": "^0.428.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typed-document-node": "^5.0.9",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@rsbuild/core": "^1.0.1-beta.14",
    "@rsbuild/plugin-react": "^1.0.1-beta.14",
    "@rspack/cli": "^1.0.13",
    "@rspack/core": "^1.0.0-beta.5",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-viewport": "^8.2.9",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "prettier": "^3.2.5",
    "storybook": "^8.2.9",
    "storybook-react-rsbuild": "^0.0.14",
    "tsx": "^4.17.0",
    "typescript": "^5.4.5"
  }
}
